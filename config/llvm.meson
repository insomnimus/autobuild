[properties]
needs_exe_wrapper = true

[host_machine]
system = 'windows'
cpu_family = 'x86_64'
cpu = 'sandybridge'
endian = 'little'

[binaries]
c = 'ab-clang'
cpp = 'ab-clang++'
ld = 'ab-lld'
ar = 'llvm-ar'
nm = 'llvm-nm'
ranlib = 'llvm-ranlib'
windres = 'ab-llvm-windres'
strip = 'ab-llvm-strip'
pkgconfig = 'x86_64-w64-mingw32-pkg-config'
pkg-config = 'x86_64-w64-mingw32-pkg-config'
cmake = 'cmake'
